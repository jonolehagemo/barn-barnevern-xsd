<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2006 rel. 3 sp2 (http://www.altova.com) by Statistics Norway (Statistics Norway) -->
<!-- Bufdir Kommunerapportering -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.11.1">
	<!-- ATTRIBUTTER -->
	<xs:attribute name="Id">
		<xs:annotation>
			<xs:appinfo>Database-ID</xs:appinfo>
			<xs:documentation>Globalt attributt for ID, f.eks databaseID/surrogatnøkkel i fagsystemet</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="MigrertId">
		<xs:annotation>
			<xs:appinfo>Database-ID</xs:appinfo>
			<xs:documentation>Globalt attributt for ID for system det er migrert fra, og som er benyttet i tidligere KOSTRA-rapportering</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="StartDato" type="xs:date">
		<xs:annotation>
			<xs:appinfo>Startdato</xs:appinfo>
			<xs:documentation>Globalt attributt for startdato</xs:documentation>
		</xs:annotation>
	</xs:attribute>
	<xs:attribute name="SluttDato" type="xs:date">
		<xs:annotation>
			<xs:appinfo>Sluttdato</xs:appinfo>
			<xs:documentation>Globalt attributt for sluttdato</xs:documentation>
		</xs:annotation>
	</xs:attribute>
	<xs:attribute name="Presisering">
		<xs:annotation>
			<xs:documentation>
        Globalt attributt for presisering, brukes i forbindelse der en overliggende kode må presiseres
      </xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="300"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<!-- ELEMENTER -->
	<xs:element name="Barnevern" type="BarnevernType">
		<xs:annotation>
			<xs:documentation>
For innsending av barnevernsdata
			</xs:documentation>
		</xs:annotation>
	</xs:element>
	<!-- TYPEDEFINISJONER -->
	<xs:complexType name="BarnevernType">
		<xs:sequence>
			<xs:element name="Fagsystem" type="FagsystemType">
				<xs:annotation>
					<xs:documentation>Informasjon om fagsystem det rapporteres fra</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Avgiver" type="AvgiverType">
				<xs:annotation>
					<xs:documentation>Kommune som rapporterer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Sak" type="SakType">
				<xs:annotation>
					<xs:documentation>Saksmappe for et barn. Komplett innhold av mappe rapporteres når innhold er endret siden siste innsending</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="DatoUttrekk" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:appinfo>
          Dato og klokkeslett for uttrekket
        </xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FagsystemType">
		<xs:attribute name="Leverandor" use="required">
			<xs:annotation>
				<xs:appinfo>Leverandøren til fagsystemer, f.eks. Visma, Acos eller NetCompany</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="3"/>
					<xs:maxLength value="300"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Navn" use="required">
			<xs:annotation>
				<xs:appinfo>Navnet til fagsystemet, f.eks. Familia</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="300"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Versjon" use="required">
			<xs:annotation>
				<xs:appinfo>Versjonen til fagsystemet, f.eks. v1.2.3</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="100"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AvgiverType">
		<xs:attribute name="Organisasjonsnummer" use="required">
			<xs:annotation>
				<xs:documentation>
          Organisasjonsnummeret til kommunen som er avgiver for dataene
        </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="9"/>
					<xs:maxLength value="9"/>
					<xs:pattern value="\d{9}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Kommunenummer" use="required">
			<xs:annotation>
				<xs:documentation>
          4-sifret kommunenummer
        </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="4"/>
					<xs:maxLength value="4"/>
					<xs:pattern value="\d{4}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Kommunenavn" use="required">
			<xs:annotation>
				<xs:documentation>Kommunens navn
        </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="250"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="SakType">
		<xs:annotation>
			<xs:documentation>
        Typedefinisjon for Sak (tidl. Individ)
      </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Virksomhet" type="VirksomhetType" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute ref="Id" use="required"/>
		<xs:attribute ref="MigrertId" use="optional"/>
		<xs:attribute ref="StartDato" use="required"/>
		<xs:attribute ref="SluttDato"/>
		<xs:attribute name="Journalnummer" use="required">
			<xs:annotation>
				<xs:documentation>
          Journal-, registrerings-, saks- eller klientnummer
        </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="20"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Fodselsnummer" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="11"/>
					<xs:maxLength value="11"/>
					<xs:pattern value="\d{11}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="DUFnummer" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="12"/>
					<xs:maxLength value="12"/>
					<xs:pattern value="\d{12}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Saksbehandler" use="required">
			<xs:annotation>
				<xs:documentation>Navn eller epost</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Avsluttet" type="xs:boolean"/>
	</xs:complexType>
	<!-- Plan -->
	<xs:complexType name="PlanType">
		<xs:sequence>
			<xs:element name="Evaluering" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:attribute name="UtfortDato" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Konklusjon" minOccurs="0">
				<xs:complexType>
					<xs:attribute ref="SluttDato" use="required">
						<xs:annotation>
							<xs:documentation>
          Dato for avslutning av planen
        </xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute ref="Id" use="required"/>
		<xs:attribute ref="MigrertId" use="optional"/>
		<xs:attribute ref="StartDato" use="required">
			<xs:annotation>
				<xs:documentation>
          Dato hvor planen regnes som aktiv
        </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Plantype" use="required">
			<xs:annotation>
				<xs:documentation>
1 = Tiltaksplan etter § 4-5
2 = Tiltaksplan etter § 4-28
3 = Foreløpig omsorgsplan etter § 4-15, 3. ledd
4 = Omsorgsplan etter § 4-15, 3. ledd
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="2"/>
					<xs:enumeration value="1">
						<xs:annotation>
							<xs:appinfo>Tiltaksplan etter § 4-5</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="2">
						<xs:annotation>
							<xs:appinfo>Tiltaksplan etter § 4-28</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="3">
						<xs:annotation>
							<xs:appinfo>Foreløpig omsorgsplan etter § 4-15, 3. ledd</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="4">
						<xs:annotation>
							<xs:appinfo>Omsorgsplan etter § 4-15, 3. ledd</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<!-- Melding -->
	<xs:complexType name="MeldingType">
		<xs:sequence>
			<xs:element name="Melder" type="MelderType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Saksinnhold" type="SaksinnholdType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Konklusjon" minOccurs="0">
				<xs:complexType>
					<xs:attribute ref="SluttDato" use="required">
						<xs:annotation>
							<xs:documentation>
          Konklusjonsdato
        </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Kode" use="required">
						<xs:annotation>
							<xs:documentation>
 1 = Henlagt av andre grunner
 1.1 = Henlagt, Utenfor BVL
 1.2 = Henlagt, Vist til annen instans
 1.3 = Henlagt, Overført til annen kommune
 2 = Ikke henlagt – konklusjonsdato melding (eventuelt 7 dager etter mottatt melding) er startdato undersøkelse
 3 = Henlagt pga. aktive tiltak
 4 = Melding i pågående undersøkelse
 				</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:list>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="3"/>
										<xs:enumeration value="1">
											<xs:annotation>
												<xs:appinfo>Henlagt av andre grunner</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="1.1">
											<xs:annotation>
												<xs:appinfo>Henlagt, Utenfor BVL</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="1.2">
											<xs:annotation>
												<xs:appinfo>Henlagt, Vist til annen instans</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="1.3">
											<xs:annotation>
												<xs:appinfo>Henlagt, Overført til annen kommune</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="2">
											<xs:annotation>
												<xs:appinfo>Ikke henlagt – konklusjonsdato melding (eventuelt 7 dager etter mottatt melding) er startdato undersøkelse</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="3">
											<xs:annotation>
												<xs:appinfo>Henlagt pga. aktive tiltak</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="4">
											<xs:annotation>
												<xs:appinfo>Melding i pågående undersøkelse</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
									</xs:restriction>
								</xs:simpleType>
							</xs:list>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute ref="Id" use="required"/>
		<xs:attribute ref="MigrertId" use="optional"/>
		<xs:attribute ref="StartDato" use="required">
			<xs:annotation>
				<xs:documentation>
          Mottaksdato for meldingen
        </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- Tiltak -->
	<xs:complexType name="TiltakType">
		<xs:sequence>
			<xs:element name="Lovhjemmel" type="LovhjemmelType"/>
			<xs:element name="JmfrLovhjemmel" type="LovhjemmelType" minOccurs="0" maxOccurs="3"/>
			<xs:element name="Kategori" type="KategoriType"/>
			<xs:element name="Tilsyn" type="TilsynType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tilsyn i fosterhjem utført av uavhengig part</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Oppfolging" type="OppfolgingType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Ansvarlig barneverntjenes oppfølgingsbesøk i fosterhjem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Opphevelse" type="OpphevelseType" minOccurs="0"/>
			<xs:element name="Konklusjon" minOccurs="0">
				<xs:complexType>
					<xs:attribute ref="SluttDato" use="required">
						<xs:annotation>
							<xs:documentation>Avslutningsdato for tiltaket
        </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Flytting">
						<xs:annotation>
							<xs:documentation>
1 = Barnevernstjenesten avslutter kontrakt med fosterhjem
1.1 = Barnet tilbakeføres til foreldre/ omsorgsplasseringen opphører
1.2 = Barnet blir myndig
1.3 = Fosterforeldre klarer ikke å dekke barnets behov (forhold ved fosterhjemmet)
1.4 = Barnet har behov for annet plasseringssted (institusjon, TFCO-fosterhjem, forsterket fosterhjem, osv. =forhold ved barnet)
1.5 = Andre grunner; spesifiser. (f.eks. uenighet om oppdragets omfang, økonomi, forsterkningstiltak mv.).
2 = Fosterforeldre avslutter kontrakten med barnevernet
2.1 = Barnet har behov fosterforeldre ikke kan dekke
2.2 = Endring i fosterforeldres livssituasjon (skilsmisse, død, osv.)
2.3 = Andre grunner; spesifiser (f.eks. uenighet om oppdragets omfang, økonomi, forsterkningstiltak, manglende eller lite effektiv veiledning, mv.).
 				</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="1">
									<xs:annotation>
										<xs:appinfo>Barnevernstjenesten avslutter kontrakt med fosterhjem</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="1.1">
									<xs:annotation>
										<xs:appinfo>Barnet tilbakeføres til foreldre/ omsorgsplasseringen opphører</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="1.2">
									<xs:annotation>
										<xs:appinfo>Barnet blir myndig</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="1.3">
									<xs:annotation>
										<xs:appinfo>Fosterforeldre klarer ikke å dekke barnets behov (forhold ved fosterhjemmet)</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="1.4">
									<xs:annotation>
										<xs:appinfo>Barnet har behov for annet plasseringssted (institusjon, TFCO-fosterhjem, forsterket fosterhjem, osv. =forhold ved barnet)</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="1.5">
									<xs:annotation>
										<xs:appinfo>Andre grunner; spesifiser. (f.eks. uenighet om oppdragets omfang, økonomi, forsterkningstiltak mv.).</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="2">
									<xs:annotation>
										<xs:appinfo>Fosterforeldre avslutter kontrakten med barnevernet</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="2.1">
									<xs:annotation>
										<xs:appinfo>Barnet har behov fosterforeldre ikke kan dekke</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="2.2">
									<xs:annotation>
										<xs:appinfo>Endring i fosterforeldres livssituasjon (skilsmisse, død, osv.)</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="2.3">
									<xs:annotation>
										<xs:appinfo>Andre grunner; spesifiser (f.eks. uenighet om oppdragets omfang, økonomi, forsterkningstiltak, manglende eller lite effektiv veiledning, mv.).</xs:appinfo>
									</xs:annotation>
								</xs:enumeration>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute ref="Id" use="required"/>
		<xs:attribute ref="MigrertId" use="optional"/>
		<xs:attribute ref="StartDato" use="required">
			<xs:annotation>
				<xs:documentation>
          Iverksatt dato for tiltaket
        </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- Lovhjemmel -->
	<xs:complexType name="LovhjemmelType">
		<xs:sequence>
			<xs:element name="Lov">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Kapittel">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Paragraf">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Ledd" maxOccurs="unbounded">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Punktum" minOccurs="0" maxOccurs="unbounded"/>
														</xs:sequence>
														<xs:attribute name="Kode" use="required"/>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="Kode" use="required"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="Kode" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Kode" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Melder -->
	<xs:complexType name="MelderType">
		<xs:attribute name="Kode" use="required">
			<xs:annotation>
				<xs:documentation>
1 = Barnet selv
2 = Mor/ far/ foresatte
3 = Familie for øvrig
4 = Andre privatpersoner
5 = Barnvernstjenesten
6 = NAV (kommune og stat)
7 = Barnevernsvakt
8 = Politi
9 = Barnehage
10 = Helsestasjon/skolehelsetjenesten
11 = Skole
12 = Pedagogisk-psykologisk tjeneste (PPT)
13 = Psykisk helsevern for barn og unge (kommune og stat)
14 = Psykisk helsevern for voksne (kommune og stat)
15 = Lege/ sykehus/ tannlege
16 = Familievernkontor
17 = Tjenester og instanser med ansvar for oppfølging av personers rusproblemer (kommune og stat)
18 = Krisesenter
19 = Asylmottak/ UDI/ innvandringsmyndighet
20 = Utekontakt/ fritidsklubb
21 = Frivillige organisasjoner/ idrettslag
22 = Andre offentlige instanser (krever presisering)
23 = Anonym
 				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:list>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="2"/>
							<xs:enumeration value="1">
								<xs:annotation>
									<xs:appinfo>Barnet selv</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="2">
								<xs:annotation>
									<xs:appinfo>Mor/ far/ foresatte</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="3">
								<xs:annotation>
									<xs:appinfo>Familie for øvrig</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="4">
								<xs:annotation>
									<xs:appinfo>Andre privatpersoner</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="5">
								<xs:annotation>
									<xs:appinfo>Barnvernstjenesten</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="6">
								<xs:annotation>
									<xs:appinfo>NAV (kommune og stat)</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
